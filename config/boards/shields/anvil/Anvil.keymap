        legacy_layer {
            bindings = <
                &none  &none  &none     &none     &none     &none   &none  &none  &none  &none  &none  &none  &none  &none  &none
                &none  &none  &none     &none     &none     &none   &none  &none  &none  &none  &none  &none  &none  &none  &none
                &none  &none  &tog DVO  &tog QWE  &tog WOR  &trans  &none  &none  &none  &none  &none  &none  &none  &none  &none
                &none  &none  &none     &none     &none     &none   &none  &none  &none  &none  &none  &none  &none  &none  &none
                &none  &none  &none     &none     &none     &none   &none  &none  &none  &none  &none  &none  &none  &none  &none
            >;
        };
